<div class="main">
  <%= form_for @user, url: users_path do |f| %>
    <%#= form_error(@user) %>
    
    <%= f.label :ニックネーム %>
    <%= f.text_field :name %>
    
    <%= f.label :メールアドレス %>
    <%= f.text_field :mail %>
    
    <%= f.label :パスワード %>
    <%= f.text_field :password %>
    
    <%= f.label :再度パスワード入力 %>
    <%= f.text_field :password_confirmation %>
    
    <%= f.submit "登録" %>
  <% end %>    
</div>